## main site
## container name: main
server {
		listen 443;
        server_name flyboard.konstantinov.com.ru;
        root /www/flyboard;
        ssl    on;
        ssl_certificate    /etc/ssl/konstantinov.com.ru.crt;
        ssl_certificate_key    /etc/ssl/konstantinov.com.ru.key;
        index index.html index.htm;      
        access_log /var/log/nginx/access.log;
}
